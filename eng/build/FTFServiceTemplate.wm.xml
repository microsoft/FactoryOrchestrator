<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<identity
    xmlns="urn:Microsoft.CompPlat/ManifestSchema.v1.00"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    >
  <service
      description="Factory Test Framework Service"
      displayName="Factory Test Framework Service"
      errorControl="normal"
      imagePath="%systemroot%\system32\manufacturing\FTFService\FTFService.exe action:run"
      name="FTFService"
      objectName="LocalSystem"
      start="auto"
      type="win32OwnProcess"
      >
    <failureActions resetPeriod="1000">
      <actions>
        <action
            delay="5"
            type="restartService"
            />
        <action
            delay="5"
            type="restartService"
            />
        <action
            delay="5"
            type="restartService"
            />
        <action
            delay="5"
            type="restartService"
            />
      </actions>
    </failureActions>
  </service>
  <firewallRule
    Protocol="TCP"
    action="Allow"
    active="True"
    binary="%systemroot%\system32\manufacturing\FTFService\FTFService.exe"
    desc="Allow outbound TCP traffic from FTFService"
    dir="Out"
    group="FTFService"
    internalName="FTFService-Out-TCP"
    name="FTFService_Allow_Out_TCP"
    />
  <firewallRule
    Protocol="TCP"
    action="Allow"
    active="True"
    binary="%systemroot%\system32\manufacturing\FTFService\FTFService.exe"
    desc="Allow inbound TCP traffic from FTFService"
    dir="In"
    group="FTFService"
    internalName="FTFService-In-TCP"
    name="FTFService_Allow_In_TCP"
    />
  <files>
  </files>
</identity>